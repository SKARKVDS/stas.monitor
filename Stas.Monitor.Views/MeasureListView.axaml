<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="80"
             x:Class="Stas.Monitor.Views.MeasureListView" Margin="5">
    <StackPanel Background="Black" Name="Container">
        <Grid ColumnDefinitions="200,350" RowDefinitions="40,40">
            <TextBlock Grid.Column="0" Grid.Row="0" Grid.RowSpan="2" Name="MeasureValueView" FontSize="40" Foreground="White" TextAlignment="Center" VerticalAlignment="Center"/>
            <TextBlock Grid.Column="1" Grid.Row="0" Name="DateView" FontSize="12" Foreground="White"/>
            <TextBlock Grid.Column="1" Grid.Row="1" Name="ExpectedValueView" FontSize="15" Foreground="White"/>
        </Grid>
    </StackPanel>
</UserControl>
